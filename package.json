{
  "name": "antlr4-helper",
  "version": "1.0.32",
  "description": "A NPM library that assists in various Antlr4 parse, language analysis, and language manipulation tasks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha",
    "build-docs": "typedoc --theme markdown --mdHideSources --out docs --includes ./src",
    "build-tinyc-sample": "antlr4-tool -o ./samples/tinyc/parser grammars/tinyc/Tinyc.g4",
    "build-json-sample": "antlr4-tool -o ./samples/json/parser grammars/json/JSON.g4 ",
    "clean": "rm -rf dist",
    "lint": "tslint -p . -c tslint.json 'src/**/*.ts'",
    "build": "tsc && antlr4-tool -o ./test/tinyc grammars/tinyc/Tinyc.g4"
  },
  "files": [
    "dist",
    "LICENSE",
    "docs"
  ],
  "keywords": [
    "ANTLR4",
    "parser",
    "antlr4",
    "antlr4-helper",
    "antlr-helper",
    "helper",
    "es5",
    "es6",
    "language",
    "dsl",
    "manipulation",
    "language-manipulation",
    "typescript"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcchatman8009/antlr4-helper"
  },
  "author": {
    "name": "Marlon Chatman",
    "email": "mcchatman8009@gmail.com"
  },
  "dependencies": {
    "antlr4": "^4.7.1",
    "lodash": "^4.17.10",
    "rxjs": "^6.2.2",
    "text-manipulation": "^1.0.6"
  },
  "devDependencies": {
    "antlr4-tool": "^1.0.17",
    "@types/node": "^10.5.4",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typedoc-plugin-markdown": "^1.1.13",
    "typescript": "^2.9.2"
  }
}
